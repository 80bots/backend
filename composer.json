{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "fideloper/proxy": "^4.0",
        "graham-campbell/github": "^8.2",
        "illuminate/routing": "^5.8",
        "jenssegers/mongodb": "^3.5",
        "laravel/cashier": "^10.0",
        "laravel/framework": "^6.18",
        "laravel/horizon": "^3.2.7",
        "laravel/passport": "^7.4",
        "laravel/tinker": "^2.0",
        "lavary/crunz": "^2.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "mongodb/mongodb": "^1.4",
        "nubs/random-name-generator": "^2.1",
        "php-http/guzzle6-adapter": "1.1.1",
        "predis/predis": "^1.1",
        "sentry/sentry-laravel": "^1.6",
        "stripe/stripe-php": "^6.41",
        "laravel/ui": "^2.0"
    },
    "require-dev": {
        "aws/aws-sdk-php-laravel": "~3.0",
        "barryvdh/laravel-ide-helper": "^2.6",
        "fzaninotto/faker": "^1.4",
        "guzzlehttp/guzzle": "~5.3|~6.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0",
        "facade/ignition": "^1.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "github-oauth": []
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/Helpers/CommonHelper.php",
            "app/Helpers/InstanceHelper.php"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ]
    }
}