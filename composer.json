{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "devdojo/chatter": "dev-master",
        "fideloper/proxy": "^4.0",
        "illuminate/routing": "^5.8",
        "laravel/cashier": "^9.3",
        "laravel/framework": "5.8.*",
        "laravel/horizon": "^3.2",
        "laravel/passport": "^7.3",
        "laravel/tinker": "^1.0",
        "lavary/crunz": "^2.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "nubs/random-name-generator": "^2.1",
        "predis/predis": "^1.1",
        "stripe/stripe-php": "^6.41"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/palashCItobuz/chatter"
        }
    ],
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.5",
        "guzzlehttp/guzzle": "~5.3|~6.0",
        "aws/aws-sdk-php-laravel": "~3.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "github-oauth": {}
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/Helpers/CommonHelper.php",
            "app/Helpers/InstanceHelper.php"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
